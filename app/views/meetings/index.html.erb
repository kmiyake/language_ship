<h1>Listing meetings</h1>

<table>
  <tr>
    <th>User</th>
    <th>Date</th>
    <th>Start time</th>
    <th>End time</th>
    <th>Location</th>
    <th>Address</th>
    <th>Message</th>
    <th>Teach language</th>
    <th>Study language</th>
    <th>Success</th>
    <th>Close</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @meetings.each do |meeting| %>
  <tr>
    <td><%= meeting.user_id %></td>
    <td><%= meeting.date %></td>
    <td><%= meeting.start_time %></td>
    <td><%= meeting.end_time %></td>
    <td><%= meeting.location %></td>
    <td><%= meeting.address %></td>
    <td><%= meeting.message %></td>
    <td><%= meeting.teach_language %></td>
    <td><%= meeting.study_language %></td>
    <td><%= meeting.success %></td>
    <td><%= meeting.close %></td>
    <td><%= link_to 'Show', meeting %></td>
    <td><%= link_to 'Edit', edit_meeting_path(meeting) %></td>
    <td><%= link_to 'Destroy', meeting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Meeting', new_meeting_path %>
